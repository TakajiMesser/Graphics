<dock:DockableContent x:Class="SauceEditor.Controls.PropertyPanel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SauceEditor.Controls"
        xmlns:dock="clr-namespace:DockingLibrary;assembly=DockingLibrary"
        mc:Ignorable="d" Title="Properties">
    <StackPanel Orientation="Vertical">
        <Label Name="Title" FontSize="16"/>
        <local:TransformPanel x:Name="PositionTransform" Grid.Row="0" Label="Position"/>
        <local:TransformPanel x:Name="RotationTransform" Grid.Row="1" Label="Rotation"/>
        <local:TransformPanel x:Name="ScaleTransform" Grid.Row="2" Label="Scale"/>
    </StackPanel>
    <!--<TreeView Name="Tree">
        
    </TreeView>-->
</dock:DockableContent>
