<dock:DockableContent x:Class="SauceEditor.Controls.DockableGamePanel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SauceEditor.Controls"
        xmlns:dock="clr-namespace:DockingLibrary;assembly=DockingLibrary" 
        mc:Ignorable="d"
        Title="DockableGameWindow" Loaded="OnLoaded">
    <DockPanel Name="MainDock">
        <ToolBar DockPanel.Dock="Top">
            <Button Command="Undo" Content="Undo"/>
            <Button Command="Redo" Content="Redo"/>
        </ToolBar>
    </DockPanel>
</dock:DockableContent>
